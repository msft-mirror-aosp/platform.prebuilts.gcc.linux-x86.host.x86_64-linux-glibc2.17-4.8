filegroup {
    name: "gcc.lib",
    srcs: [
      "lib/**/*",
      "x86_64-linux/lib/**/*",
    ],
}
filegroup {
    name: "gcc.lib32",
    srcs: [
      "x86_64-linux/lib32/**/*",
